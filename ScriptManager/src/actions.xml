<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
	<ACTION NAME="sm.execute">
		<CODE>ScriptManager.execute();</CODE>
	</ACTION>
	<ACTION NAME="sm.ruby">
		<CODE>ScriptManager.ruby();</CODE>
	</ACTION>
	<ACTION NAME="sm.select">
		<CODE>ScriptManager.select();</CODE>
	</ACTION>
	<ACTION NAME="sm.current">
		<CODE>ScriptManager.selectCurrent();</CODE>
	</ACTION>
</ACTIONS>
