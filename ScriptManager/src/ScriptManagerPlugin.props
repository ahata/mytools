# Properties file for QuickNotepad plugin
# $Id: QuickNotepad.props 6876 2006-09-02 20:39:49Z jchoyt $

# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.ScriptManagerPlugin.activate=defer

# These two properties are required for all plugins
plugin.ScriptManagerPlugin.name=ScriptManager
plugin.ScriptManagerPlugin.author=zhao

# version number == jEdit version number
plugin.ScriptManagerPlugin.version=1.1

# online help
plugin.ScriptManagerPlugin.docs=index.html

# we only have one dependency, jEdit 4.2, since we use the new plugin API
plugin.ScriptManagerPlugin.depend.0=jedit 04.02.01.00
plugin.ScriptManagerPlugin.depend.1=jdk 1.5
plugin.ScriptManagerPlugin.jars=commons-lang-2.4.jar commons-io-1.4.jar

plugin.ScriptManagerPlugin.description=Script Manager
# plugin menu
plugin.ScriptManagerPlugin.menu= \
	sm.execute \
	sm.select \
	sm.replace \
	sm.current \
	sm.ruby

# action labels for actions supplied by actions.xml
sm.execute.label=exec1
sm.replace.label=exec2
sm.select.label=select
sm.current.label=current
sm.ruby.label=ruby

# action labels for actions supplied by dockables.xml
manager.label=Script Manager

# window title
manager.title=Script Manager

